    <body>
      
        <header class="top" id="home">
           <div class="background-img"> <img src="resources/img/a4.jpg" alt="white" class="img-responsive trans"> </div>
            <div class="home-text">
                <h1>SAVED?</h1>
            </div>
        </header>
        
