                <% if(currentUser){ %>
                  <li><a href="/<%= currentUser.username%>"><%=currentUser.username%></a> </li>
                  <li><a href="/logout">logout</a></li>
                <% } else{ %>
                  <li><a href="/register"><span class="glyphicon glyphicon-user"></span> Sign Up</a></li>
